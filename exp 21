    <frame src="data:text/html;charset=UTF-8,<html><head><title>Top Frame</title></head><body><h1>This is the Top Frame</h1><p>Welcome to the top frame of the page!</p></body></html>" name="topFrame" />
    <frame src="data:text/html;charset=UTF-8,<html><head><title>Bottom Frame</title></head><body><h1>This is the Bottom Frame</h1><p>Welcome to the bottom frame of the page!</p></body></html>" name="bottomFrame" />
</frameset>
</html>
